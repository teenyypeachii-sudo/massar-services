(function(n,t){var i={min:n.validator.methods.min,max:n.validator.methods.max,range:n.validator.methods.range};n.validator.methods.number=function(i,r){var u=t.parseFloat(i);return this.optional(r)||n.isNumeric(u)};n.validator.methods.date=function(n,i){var r=t.parseDate(n);return this.optional(i)||r};n.validator.methods.min=function(n,r,u){var f=t.parseFloat(n);return i.min.call(this,f,r,u)};n.validator.methods.max=function(n,r,u){var f=t.parseFloat(n);return i.max.call(this,f,r,u)};n.validator.methods.range=function(n,r,u){var f=t.parseFloat(n);return i.range.call(this,f,r,u)}})(jQuery,Globalize);$.validator.addMethod("requiredif",function(n,t,i){var u=(i.desiredvalue==null?"":i.desiredvalue).toString(),r={},f=$('input[name="'+i.dependentproperty+'"]').attr("type");return(r=f=="checkbox"?$('input[name="'+i.dependentproperty+'"]').prop("checked"):$('input[name="'+i.dependentproperty+'"]').length>0?$('input[name="'+i.dependentproperty+'"]').val():$('select[name="'+i.dependentproperty+'"]').val(),$.trim(u).toLowerCase()===$.trim(r).toLocaleLowerCase())?$.validator.methods.required.call(this,n,t,i):!0});$.validator.unobtrusive.adapters.add("requiredif",["dependentproperty","desiredvalue"],function(n){n.rules.requiredif=n.params;n.messages.requiredif=n.message})